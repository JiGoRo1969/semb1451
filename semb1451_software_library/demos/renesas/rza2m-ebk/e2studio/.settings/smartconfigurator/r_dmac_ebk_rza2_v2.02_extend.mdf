      <property id="DMAC_SOURCE_UNIT" display="Transfer unit size (source)" default="DMA_DATA_SIZE_1" type="combobox">
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_1" display="1 byte" value="DMA_DATA_SIZE_1"/>
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_2" display="2 byte" value="DMA_DATA_SIZE_2"/>
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_4" display="4 byte" value="DMA_DATA_SIZE_4"/>
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_8" display="8 byte" value="DMA_DATA_SIZE_8"/>
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_16" display="16 byte" value="DMA_DATA_SIZE_16"/>
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_32" display="32 byte" value="DMA_DATA_SIZE_32"/>
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_64" display="64 byte" value="DMA_DATA_SIZE_64"/>
        <option id="DMAC_SOURCE_UNIT.DMA_DATA_SIZE_128" display="128 byte" value="DMA_DATA_SIZE_128"/>
        <description>DMA transfer unit size (source)</description>
      </property>
      <property id="DMAC_DESTINATION_UNIT" display="Transfer unit size (destination)" default="DMA_DATA_SIZE_1" type="combobox">
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_1" display="1 byte" value="DMA_DATA_SIZE_1"/>
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_2" display="2 byte" value="DMA_DATA_SIZE_2"/>
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_4" display="4 byte" value="DMA_DATA_SIZE_4"/>
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_8" display="8 byte" value="DMA_DATA_SIZE_8"/>
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_16" display="16 byte" value="DMA_DATA_SIZE_16"/>
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_32" display="32 byte" value="DMA_DATA_SIZE_32"/>
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_64" display="64 byte" value="DMA_DATA_SIZE_64"/>
        <option id="DMAC_DESTINATION_UNIT.DMA_DATA_SIZE_128" display="128 byte" value="DMA_DATA_SIZE_128"/>
        <description>DMA transfer unit size (destination)</description>
      </property>
      <property id="DMAC_ADDR_TYPE_SOURCE" display="Address type (source)" default="DMA_ADDRESS_INCREMENT" type="combobox">
        <option id="DMAC_ADDR_TYPE_SOURCE.DMA_ADDRESS_INCREMENT" display="Increment" value="DMA_ADDRESS_INCREMENT"/>
        <option id="DMAC_ADDR_TYPE_SOURCE.DMA_ADDRESS_FIX" display="Fix" value="DMA_ADDRESS_FIX"/>
        <description>DMA address type (source)</description>
      </property>
      <property id="DMAC_ADDR_TYPE_DESTINATION" display="Address type (destination)" default="DMA_ADDRESS_INCREMENT" type="combobox">
        <option id="DMAC_ADDR_TYPE_DESTINATION.DMA_ADDRESS_INCREMENT" display="Increment" value="DMA_ADDRESS_INCREMENT"/>
        <option id="DMAC_ADDR_TYPE_DESTINATION.DMA_ADDRESS_FIX" display="Fix" value="DMA_ADDRESS_FIX"/>
        <description>DMA address type (destination)</description>
      </property>
      <property id="DMAC_DIRECTION" display="Transfer direction" default="DMA_REQUEST_SOURCE" type="combobox">
        <option id="DMAC_DIRECTION.DMA_REQUEST_SOURCE" display="Read" value="DMA_REQUEST_SOURCE"/>
        <option id="DMAC_DIRECTION.DMA_REQUEST_DESTINATION" display="Write" value="DMA_REQUEST_DESTINATION"/>
        <description>DMA transfer direction</description>
      </property>
      <property id="DMAC_COMPLETE_CALLBACK_FUNCTION" display="Callback function (DMA end interrupt)" default="NULL" type="textbox">
        <constraint display="Callback function is not allowed to be empty, it must be a valid function name or NULL" actionOnFail="error">(/^[a-zA-Z_]\w*$/.test("${DMAC_COMPLETE_CALLBACK_FUNCTION}") || ("${DMAC_COMPLETE_CALLBACK_FUNCTION}" == "NULL"))</constraint>
        <description>Set callback function (DMA end interrupt), set NULL if not use callback function</description>
      </property>
      <property id="DMAC_ERROR_CALLBACK_FUNCTION" display="Callback function (DMA error)" default="NULL" type="textbox">
        <constraint display="Callback function is not allowed to be empty, it must be a valid function name or NULL" actionOnFail="error">(/^[a-zA-Z_]\w*$/.test("${DMAC_ERROR_CALLBACK_FUNCTION}") || ("${DMAC_ERROR_CALLBACK_FUNCTION}" == "NULL"))</constraint>
        <description>Set callback function (DMA error), set NULL if not use callback function</description>
      </property>
      <property id="DMAC_SOURCE_ADDR" display="Source address" default="0x00000000" type="textbox">
        <constraint display="Value must be a hex integer and in the range 0~0xFFFFFFFF" actionOnFail="error">testHexInteger("${DMAC_SOURCE_ADDR}") &amp;&amp; ("${DMAC_SOURCE_ADDR}".replace(/[LUlu]+$/, '')  &gt;= 0) &amp;&amp; ("${DMAC_SOURCE_ADDR}".replace(/[LUlu]+$/, '')  &lt;= 0xffffffff)</constraint>
        <description>Set source address</description>
      </property>
      <property id="DMAC_DESTINATION_ADDR" display="Destination address" default="0x00000000" type="textbox">
        <constraint display="Value must be a hex integer and in the range 0~0xFFFFFFFF" actionOnFail="error">testHexInteger("${DMAC_DESTINATION_ADDR}") &amp;&amp; ("${DMAC_DESTINATION_ADDR}".replace(/[LUlu]+$/, '')  &gt;= 0) &amp;&amp; ("${DMAC_DESTINATION_ADDR}".replace(/[LUlu]+$/, '')  &lt;= 0xffffffff)</constraint>
        <description>Set destination address</description>
      </property>
      <property id="DMAC_DATA_LENGTH" display="Transfer size" default="1024" type="textbox">
        <constraint display="Value must be an integer" actionOnFail="error">testInteger("${DMAC_DATA_LENGTH}")</constraint>
        <constraint display="Value must be in the range 0 ~ 4294967295" actionOnFail="error">("${DMAC_DATA_LENGTH}" &gt;= 0) &amp;&amp; ("${DMAC_DATA_LENGTH}" &lt;= 4294967295)</constraint>
        <description>Length of data to transfer (uint32)</description>
      </property>
    </configuration>
</module>
